<% provide(:title, 'Add New Words to Dictionary') %>
<h1>New Word</h1>


Testing: <%= p @dictionary %>

<%= form_for(@dictionary) do |f| %>

    <%= f.label :word %>
    <%= f.text_field :word %>

    <%= f.label :description %>
    <%= f.text_field :description %>

    <%= f.submit "Add", class: "btn btn-large btn-primary" %>
<% end %>

